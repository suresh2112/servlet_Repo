<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>001ServletDynamic</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.createiq.hellodemo.Hello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello123</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>hi</servlet-name>
    <servlet-class>com.createiq.hellodemo.Hi</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hi</servlet-name>
    <url-pattern>/hi123</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>com.createiq.hellodemo.Welcome</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome123</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HiHelloWelcome</display-name>
    <servlet-name>HiHelloWelcome</servlet-name>
    <servlet-class>com.createiq.hellohiwelcome.HiHelloWelcome</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>HiHelloWelcome</servlet-name>
    <url-pattern>/HiHelloWelcome</url-pattern>
  </servlet-mapping>
</web-app>